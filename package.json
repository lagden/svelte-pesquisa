{
  "name": "@tadashi/svelte-pesquisa",
  "version": "1.3.3",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/svelte-pesquisa.git"
  },
  "description": "Svelte components",
  "keywords": [
    "svelte",
    "elements",
    "search",
    "component"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "homepage": "https://github.com/lagden/svelte-pesquisa#readme",
  "bugs": {
    "url": "https://github.com/lagden/svelte-pesquisa/issues"
  },
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "svelte": "src/Pesquisa.svelte",
  "main": "dist/Pesquisa.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "prettier": "prettier -c --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "cp": "cp dist/* public/module/.",
    "esbuild": "node ./esbuild.config.js",
    "build": "run-s test rm esbuild cp",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "_test": "jest test --coverage --coverageReporters=lcov --coverageReporters=text",
    "__test": "NODE_OPTIONS='--experimental-vm-modules --trace-warnings' jest test --env=./custom-test-env.js --coverage --coverageReporters=lcov --coverageReporters=text",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest test --env=./custom-test-env.js --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch",
    "server_": "sirv public --single --host 0.0.0.0 --dev",
    "server": "sirv public --cors --single --host 0.0.0.0 --port 5001 --dev"
  },
  "dependencies": {
    "@tadashi/flatten-object": "3.1.0",
    "@tadashi/unflatten-object": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/svelte": "3.2.1",
    "babel-jest": "29.0.3",
    "esbuild": "0.15.8",
    "esbuild-svelte": "0.7.1",
    "eslint": "8.23.1",
    "eslint-config-xo": "0.42.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "sirv-cli": "2.0.2",
    "svelte": "3.50.1",
    "svelte-jester": "2.3.2",
    "timekeeper": "2.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "packageManager": "yarn@3.2.3"
}
